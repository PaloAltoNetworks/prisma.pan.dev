          "cloud.account": { "$ref": "#/components/schemas/FilterSuggestion" },
          "cloud.region": { "$ref": "#/components/schemas/FilterSuggestion" },
          "cloud.service": { "$ref": "#/components/schemas/FilterSuggestion" },
